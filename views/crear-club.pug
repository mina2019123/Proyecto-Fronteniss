block contenido

    <!DOCTYPE html>
    html(lang="en")
        head
            meta(charset="UTF-8")
            meta(name="viewport", content="width=device-width, initial-scale=1.0")
            link(rel="stylesheet", href="../public/css/crearclubs.css")
            <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
            
            title=nombrePagina
        body
           div.container
                form(action="")
                div.row
                    div.col-4
                        <div class="form-group">
                            div.contenedorImagen
                            <input type="file" class="form-control-file" id="exampleFormControlFile1">
                        </div>
                        
                    div.col-4#formulario     
                        form(action="")
                            <div class="form-group" >
                                <label for="exampleInputEmail1">Nombre del Equipo</label>
                                <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
                            </div>
                            <div class="form-group">
                                <label for="exampleFormControlSelect1">Seleccionar categoria</label>
                                <select class="form-control" id="exampleFormControlSelect1">
                                    <option>Femenil</option>
                                    <option>Varonil</option>
                                    option tutifruti 
                                </select>
                            div.form-group
                               <button type="button"class="btn btn-primary" id="enviarInformacion">Enviar Informacion</button>
                div.container
                    div.card
                    div.card-header
                        span  Lista de Equipos
                    table.table
                        thead
                            tr
                                <th scope="col">Numero</th>
                                <th scope="col">Nombre del club</th>
                                <th scope="col">Categoria</th>
                                <th scope="col">Operaciones</th>


                    











        
